<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.2.xsd
        http://www.springframework.org/schema/aop
        https://www.springframework.org/schema/aop/spring-aop.xsd">

    <bean id="customerXml" class="io.kimmking.beandemo.model.Customer">
        <property name="id" value="11111111" />
        <property name="name" value="Leo" />
        <property name="birthday" value="2010-12-23" />
    </bean>

    <bean id="espresso" class="io.kimmking.beandemo.model.Coffee">
        <property name="name" value="Espresso" />
    </bean>

<!--    <context:component-scan base-package="io.kimmking.beandemo.componentscan" />-->
</beans>